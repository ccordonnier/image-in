{% extends 'base.html.twig' %}
{% block stylesheets %}
{% endblock %}

{% block body %}
<h1>test</h1>
<div class="container">
		<div class="grid gallery row">
  			{% for picture in pictures %}
  				<div class="grid-item col m4">
  					<div class="row">
    					<div class="col s12 m12">
                            <div class="card">
                                <div class="card-image waves-effect waves-block waves-light">
                                    <img class="activator" src="{{ asset('images/' ~ picture.name ) }}">
                                </div>
                                <div class="card-content">
                                    <span class="card-title activator grey-text text-darken-4">{{ picture.name }}</span>
                                    <p><a href="#"><i class="material-icons">visibility</i></a><a href="#"><i class="material-icons">file_download</i></a><a href="#"><i class="material-icons">edit</i></a><a href="#"><i class="material-icons">delete</i></a></p>
                                </div>
                            </div>
    					</div>
  					</div>	
  				</div>
  			{% endfor %}
		</div>
</div>

{% endblock %}

{% block javascripts %}

{% endblock %}